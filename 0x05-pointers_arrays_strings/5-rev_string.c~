#include "main.h"
/**
 * rev_string - function that reverses string
 * @s: declaring s for function
 * Return: Always 0
 */
void rev_string(char *s)
{
	int x, y;
	char z;

	for (x = '\0'; s[x] != 0; x++)
	{
	}
	y = 0;
	for (x = x - 1; y < x; y++)
	{
		z = s[y];
		s[x] = s[y];
		s[y] = z;
		x--;
	}
}
