#include "main.h"
/**
 * rev_string - function that reverses string
  * @s: declaring s for function
   * Return: Always 0
    */
    void rev_string(char *s)
    {
            int x, len, z;
	            len = len(s);

        for (len = 0; x < len / 2; x++)
	        {
		                z = s[x];
				                s[x] = s[len - x - 1];
						                s[len - x - 1] = z;
								        }
									}
									