#include "lists.h"
/**
 * print_dlistint_backward - Prints all the elements of a dlistint_t list backward.
 * @h: A pointer to the tail of the list.
 *
 * Return: The number of nodes in the list.
 */
size_t print_dlistint_backward(const dlistint_t *h)
{
    const dlistint_t *current;
    size_t count;

    if (h == NULL)
        return (0);

    /* Move the pointer to the head of the list */
    current = h;
    while (current->next != NULL)
        current = current->next;

    /* Traverse the list from the tail and print the data of each node */
    count = 0;
    while (current != NULL)
    {
        printf("%d\n", current->n);
        current = current->prev;
        count++;
    }

    return (count);
}
